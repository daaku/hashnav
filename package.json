{
  "name": "@daaku/hashnav",
  "author": "Naitik Shah <n@daaku.org>",
  "version": "5.0.0",
  "description": "Modern History API enabled hash based navigation.",
  "repository": "git@github.com:daaku/hashnav",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "eslintConfig": {
    "extends": "@daaku"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --fix --ignore-pattern dist .",
    "check": "run-p 'check:*' 'test'",
    "build:js": "tsc",
    "build:test": "esbuild --bundle --sourcemap --define:process.env.NODE_ENV=\\\"production\\\" --outdir=dist/bundle test/tests.ts",
    "test": "npm run build:test && qunit-puppeteer",
    "cover": "npm run test && c8 report",
    "build": "run-p check:lint test build:js",
    "deploy": "npm run build && npm publish --access=public",
    "prepare": "npm run build:js"
  },
  "keywords": [
    "hash",
    "history",
    "navigation"
  ],
  "devDependencies": {
    "@daaku/eslint-config": "^4.0.0",
    "@daaku/qunit-puppeteer": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "c8": "^7.11.3",
    "esbuild": "^0.18.1",
    "eslint": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.17.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "regexparam": "2.0.1"
  }
}
