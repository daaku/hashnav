{
  "name": "@daaku/hashnav",
  "author": "Naitik Shah <n@daaku.org>",
  "version": "5.4.0",
  "description": "Modern History API enabled hash based navigation.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/daaku/hashnav.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "test": "vite build && qunit-puppeteer",
    "cover": "npm run test && c8 report",
    "deploy": "npm run test && npm publish --access=public",
    "prepare": "tsc",
    "dev": "vite"
  },
  "keywords": [
    "hash",
    "history",
    "navigation"
  ],
  "devDependencies": {
    "@daaku/qunit-puppeteer": "^3.3.0",
    "@types/qunit": "^2.11.2",
    "c8": "^9.0.0",
    "qunit": "^2.17.2",
    "typescript": "^5.0.2",
    "vite": "^5.0.2"
  },
  "dependencies": {
    "regexparam": "3.0.0"
  }
}
