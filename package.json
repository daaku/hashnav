{
  "name": "@daaku/hashnav",
  "author": "Naitik Shah <n@daaku.org>",
  "version": "5.1.0",
  "description": "Modern History API enabled hash based navigation.",
  "repository": "git@github.com:daaku/hashnav",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "tsc",
    "test": "esbuild --bundle --sourcemap --outdir=dist/bundle test/tests.ts && qunit-puppeteer",
    "cover": "npm run test && c8 report",
    "deploy": "npm run test && npm publish --access=public",
    "prepare": "npm run build"
  },
  "keywords": [
    "hash",
    "history",
    "navigation"
  ],
  "devDependencies": {
    "@daaku/qunit-puppeteer": "^3.0.0",
    "@types/qunit": "^2.11.2",
    "c8": "^8.0.0",
    "esbuild": "^0.19.0",
    "qunit": "^2.17.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "regexparam": "2.0.1"
  }
}
